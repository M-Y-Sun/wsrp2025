 CellChangeTimes->{{3.96037614506467*^9, 
  3.960376166844016*^9}},ExpressionUUID->"cb5574e7-0ef1-497f-8986-\
64e558ac86a6"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"maxiters", "=", "128"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"rules", "=", 
   RowBox[{"Range", "[", 
    RowBox[{"0", ",", "4095"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"bits", "=", "4"}], ";"}]}], "Input",
 CellChangeTimes->{{3.960376171276965*^9, 3.9603761759662857`*^9}, {
