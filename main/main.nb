Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"getevols", "[", "bits_Integer", "]"}], ":=", 
    RowBox[{"ParallelTable", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Join", "@", 
       RowBox[{"{", "\[IndentingNewLine]", 
        RowBox[{"r", ",", "\[IndentingNewLine]", 
         RowBox[{"Table", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"With", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"h", "=", 
               RowBox[{"hist", "[", 
                RowBox[{"n", ",", "r"}], "]"}]}], "}"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{"If", "[", "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{
                RowBox[{"h", "[", 
                 RowBox[{"[", 
                  RowBox[{
                   RowBox[{"-", "1"}], ",", "1", ",", "3"}], "]"}], "]"}], "!=",
                 "1"}], ",", "\[IndentingNewLine]", 
               RowBox[{"-", "1"}], ",", "\[IndentingNewLine]", 
               RowBox[{"FromDigits", "[", 
                RowBox[{
                 RowBox[{"h", "[", 
                  RowBox[{"[", 
                   RowBox[{
                    RowBox[{"-", "1"}], ",", "2"}], "]"}], "]"}], ",", "2"}], 
                "]"}]}], "\[IndentingNewLine]", "]"}]}], 
            "\[IndentingNewLine]", "]"}], ",", 
           RowBox[{"(*", " ", 
            RowBox[{"end", " ", "With"}], " ", "*)"}], "\[IndentingNewLine]", 
           
           RowBox[{"{", 
            RowBox[{"n", ",", "1", ",", 
             RowBox[{
              RowBox[{"2", "^", "bits"}], "-", "1"}]}], "}"}]}], 
          "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", "}"}]}], ",",
       "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{"r", ",", "rules"}], "}"}]}], "\[IndentingNewLine]", "]"}]}], 
   ";"}], 
  RowBox[{"(*", " ", 
   RowBox[{"end", " ", "Table", " ", "over", " ", "r"}], " ", "*)"}], 
  "\[IndentingNewLine]"}]], "Input",
