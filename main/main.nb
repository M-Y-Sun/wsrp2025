Cell[BoxData[
 RowBox[{
  RowBox[{"hist", "[", 
   RowBox[{"n_", ",", " ", "rule_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"With", "[", 
