 RowBox[{
  RowBox[{"maxiters", "=", "128"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"rules", "=", 
   RowBox[{"Range", "[", 
    RowBox[{"0", ",", "4095"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"bits", "=", "4"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"width", "=", "11"}], ";"}]}], "Input",
 CellChangeTimes->{{3.960376171276965*^9, 3.9603761759662857`*^9}, {
  3.9603853710410633`*^9, 3.9603853719435387`*^9}, {3.960453267285596*^9, 
