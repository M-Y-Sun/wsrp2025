          RowBox[{"(", 
           RowBox[{
            RowBox[{"i", "++"}], ";", 
            RowBox[{"TuringMachine", "[", 
             RowBox[{"rule", ",", "#"}], "]"}]}], ")"}], "&"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"{", 
